{
    "buildCommand": "cd web && yarn install && yarn build",
    "outputDirectory": "web/dist",
    "installCommand": "yarn install",
    "framework": null,
    "ignoreCommand": "git diff HEAD^ HEAD --quiet -- archive/",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
